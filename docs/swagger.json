{
    "swagger": "2.0",
    "info": {
        "description": "This is a sample Swagger API with net/http",
        "title": "Swagger with net/http",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/username/": {
            "get": {
                "description": "return list of usernames",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "username"
                ],
                "summary": "Get all usernames",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "post": {
                "description": "add a username to list",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "username"
                ],
                "summary": "Add new username",
                "parameters": [
                    {
                        "description": "Username JSON",
                        "name": "username",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "added successfully",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}